{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "edpcmentoring-ng8": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/projectlight/images/interface",
              "src/projectlight/javascripts/custom.js",
              "src/projectlight/javascripts/custom_navonly.js",
              "src/projectlight/javascripts/libs/bootstrap-tabs.js",
              "src/projectlight/javascripts/libs/datepicker_highlighted_events.js",
              "src/projectlight/javascripts/libs/ios-orientationchange-fix.js",
              "src/projectlight/javascripts/libs/jquery-min.js",
              "src/projectlight/javascripts/libs/modernizr.js",
              "src/projectlight/javascripts/libs/responsive-tooltip.js",
              "src/projectlight/javascripts/libs/zebra_datepicker.js"
            ],
            "styles": [
              "src/styles.css",
              "src/projectlight/stylesheets/custom.css",
              "src/projectlight/stylesheets/full-stylesheet.css",
              "src/projectlight/stylesheets/styleguide.css"
            ],
            "scripts":["jquery.min.js"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.prod.ts"
              }]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "edpcmentoring-ng8:build",
            "proxyConfig": "src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "edpcmentoring-ng8:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "edpcmentoring-ng8:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.css",
              "src/projectlight/stylesheets/custom.css",
              "src/projectlight/stylesheets/full-stylesheet.css",
              "src/projectlight/stylesheets/styleguide.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/projectlight/images/interface",
              "src/projectlight/javascripts/custom.js",
              "src/projectlight/javascripts/libs/bootstrap-tabs.js",
              "src/projectlight/javascripts/libs/datepicker_highlighted_events.js",
              "src/projectlight/javascripts/libs/ios-orientationchange-fix.js",
              "src/projectlight/javascripts/libs/jquery-min.js",
              "src/projectlight/javascripts/libs/modernizr.js",
              "src/projectlight/javascripts/libs/responsive-tooltip.js",
              "src/projectlight/javascripts/libs/zebra_datepicker.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "edpcmentoring-ng8-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "edpcmentoring-ng8:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "campl-ng": {
      "projectType": "library",
      "root": "projects/campl-ng",
      "sourceRoot": "projects/campl-ng/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/campl-ng/tsconfig.lib.json",
            "project": "projects/campl-ng/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/campl-ng/src/test.ts",
            "tsConfig": "projects/campl-ng/tsconfig.spec.json",
            "karmaConfig": "projects/campl-ng/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/campl-ng/tsconfig.lib.json",
              "projects/campl-ng/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "edpcmentoring-ng8",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "men",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "men"
    }
  }
}
